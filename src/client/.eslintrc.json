{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "performance": true,
    "Stats": true,
    "THREE": true,
    "window": true
  },
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "no-bitwise": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "spaced-comment": "off"
  }
}
