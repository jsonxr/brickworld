{
  "extends": "eslint:recommended",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": "warn",
    "quotes": [
      "error",
      "single"
    ],
  }
}
